#include <stdio.h>

int main(){
	int n,i;
	printf("moi nhap n: ");
	scanf("%d",&n);
	long long s=0;
	printf("moi nhap day so: ");
	long long a[n];
	for(i=1;i<=n;i++){
		scanf("%lld",&a[i]);
	}
	printf("Day so la:\n");
	for(i=1;i<=n;i++){
		printf("%lld ",a[i]);
		int b=a[i];
		s=s+b;
	}
	printf("\nTong day so la: %lld", s);
}
